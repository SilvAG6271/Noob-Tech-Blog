<div class="container">
    <h3>{{username}}</h3>
    <a href="/newPost" class="waves-effect waves-light btn">Create a New Post</a> 
{{#each postDatas as |postData|}}
<div>
    <div>
    <a href="/singlePost/{{encodeURIComponent postData.id}}"><h2 class="post-link">{{postData.title}}</h2>
    <p>Posted: {{postData.created_on}}</p>
    </div>
    <p>{{postData.text}}</p>
</div>
{{/each}}
</div>
