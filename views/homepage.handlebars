<h1>The Noob Tech Blog</h1>
 {{#each posts}}
 <div class="row">
    <div class="col s12 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">{{title}}</span>
          <p>Posted by {{username}} on {{created_on}}.</p>
          <p>{{text}}</p>
        </div>
        <div class="card-action">
        </div>
      </div>
    </div>
  </div>
  {{/each}}